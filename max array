#include<stdio.h>
int main()
{
    int a[20],n,i,max=a[0];
    printf("\tEnter the limit:");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("\tEnter a[%d]:",i);
        scanf("%d",&a[i]);
    }
    
    for(i=0;i<n;i++)
    {
        if(a[i]>max)
        {
            max=a[i];
        }
     
     
    }
   printf("maximum=%d",max);
    return 0;
}
